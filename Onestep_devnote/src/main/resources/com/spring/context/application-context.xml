<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="NoteDAO"
		  class="com.spring.dao.NoteDAOImpl"
		  p:sqlSession-ref="sqlSession" />
		  
	<bean id="NoteService"
		  class="com.spring.service.NoteServiceImpl"
		  p:noteDAO-ref="NoteDAO"
		  
		  />
		  
	<bean id="CalendarDAO"
		  class="com.spring.dao.CalendarDAOImpl"
		  p:sqlSession-ref="sqlSession"/>
		  
	
	<bean id="ProjectDAO"
		  class="com.spring.dao.ProjectDAOImpl"
		  p:sqlSession-ref="sqlSession" 
		  />
		  
	<bean id="ProjectService" 
		  class="com.spring.service.ProjectServiceImpl"
		  p:projectDAO-ref="ProjectDAO"
		  p:calendarDAO-ref="CalendarDAO"
		  />
		
	
</beans>
